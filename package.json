{
  "name": "@untr/apps-coip",
  "main": "index.ts",
  "version": "1.0.0",
  "versionCode": 1,
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ],
    "showSeed": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@realm/fetch|@dev-plugins/react-query)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "scripts": {
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start -c",
    "android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "dev:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
    "dev:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "dev:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start -c --web",
    "build": "npx expo export -p web",
    "build:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 cd android && ./gradlew assembleRelease && cd ..",
    "clean:android": "cd android && chmod +x ./gradlew && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && pod cache clean --all && rm -rf build && cd ..",
    "serve": "npx serve dist --single",
    "clean": "rm -rf .expo node_modules && watchman watch-del-all && rm -rf package-lock.json",
    "clean:windows": "rd /s /q .expo && rd /s /q node_modules && watchman watch-del-all && del package-lock.json && del pnpm-lock.yaml",
    "generate:api": "kubb generate",
    "clean-watchman": "watchman watch-del-all",
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "postinstall": "EXPO_USE_METRO_WORKSPACE_ROOT=1 npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css && patch-package",
    "ios": "NODE_ENV=.env && expo run:ios",
    "uninstall": "adb uninstall com.unitedtractors.coipmobile",
    "lint": "eslint .",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "test": "jest --watch --coverage=false",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "dependencies": {
    "@bam.tech/react-native-app-security": "^0.5.0",
